// Place your key bindings in this file to override the defaults
[
  { "key": "ctrl+shift+space", "command": "macros.jumpLineDown" },
  { "key": "ctrl+shift+j", "command": "macros.cursorDown" },
  { "key": "ctrl+shift+h", "command": "macros.cursorLeft" },
  { "key": "ctrl+shift+l", "command": "macros.cursorRight" },
  { "key": "ctrl+shift+k", "command": "macros.cursorUp" },
  { "key": "ctrl+shift+alt+l", "command": "cursorWordEndRight" },
  { "key": "ctrl+shift+alt+h", "command": "cursorWordEndLeft" },
  { "key": "ctrl+shift+alt+k","command": "cursorMove","args": {"to": "up","by": "line","value": 10},"when": "editorTextFocus"},
  { "key": "ctrl+shift+alt+j", "command": "cursorMove","args": {"to": "down","by": "line","value": 10},"when": "editorTextFocus"},
  {"key": "tab","command": "selectNextSuggestion","when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"},
  {"key": "down","command": "-selectNextSuggestion","when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"},
  {"key": "ctrl+tab","command": "selectPrevSuggestion","when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"},
  {"key": "up",    "command": "-selectPrevSuggestion","when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"},
  {"key": "tab","command": "workbench.action.quickOpenSelectNext","when": "inQuickOpen"},
  {"key": "ctrl+tab","command": "workbench.action.quickOpenSelectPrevious","when": "inQuickOpen"},
  {
    "key": "ctrl+shift+]",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+shift+[", 
    "command": "workbench.action.navigateEditorGroups" 
  },  
  {
    "key": "space",
    "command": "workbench.action.quickOpenNavigateNextInViewPicker",
    "when": "inQuickOpen"
  },
  {
    "key": "shift+alt+down",
    "command": "editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+alt+down",
    "command": "-editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  }
]
